<?xml version="1.0"?>
<Maths Locale="English_United States.1252" DefaultLocale="C" Id="Suspension">
 <MathConstants>
  <MathConstant Name="CG Height" Value="330" Unit="mm"/>
  <MathConstant Name="Damper Smoothing" Value="5" Unit="" QuickEdit="1"/>
  <MathConstant Name="damper stroke" Value="47" Unit="mm"/>
  <MathConstant Name="Front Heave Damper Unloaded Length" Value="189" Unit="mm"/>
  <MathConstant Name="Front Roll Damper Unloaded Length" Value="189" Unit="mm"/>
  <MathConstant Name="Front Spring Stiffenss" Value="50" Unit="N/mm"/>
  <MathConstant Name="Front track" Value="1245" Unit="mm"/>
  <MathConstant Name="Front Weight Bias " Value="37.3" Unit="%"/>
  <MathConstant Name="Motion Ratio" Value="1.05" Unit=""/>
  <MathConstant Name="Rear Heave Damper Unloaded Length" Value="189" Unit="mm"/>
  <MathConstant Name="Rear Roll Damper Unloaded Length" Value="189" Unit="mm"/>
  <MathConstant Name="Rear Spring Stiffness" Value="50" Unit="N/mm"/>
  <MathConstant Name="Rear track" Value="1245" Unit="mm"/>
  <MathConstant Name="Tire Radius" Value="10" Unit="in"/>
  <MathConstant Name="Vehicle Mass" Value="313.6" Unit="kg"/>
  <MathConstant Name="Wheelbase" Value="1530" Unit="mm"/>
 </MathConstants>
 <MathItems>
  <MathExpression Id="Front roll angle" DisplayUnit="deg" DisplayDPS="2" DisplayColorIndex="1" Interpolate="1" Script="atan(( 'FL Wheel movement' [mm] - 'FR Wheel movement' [mm] ) / 'Front track' [mm])" SampleRate="0" Unit="rad"/>
  <MathExpression Id="Rear roll angle" DisplayUnit="deg" DisplayDPS="2" DisplayColorIndex="6" Interpolate="1" Script="atan(( 'RL Wheel movement' [mm] - 'RR Wheel movement' [mm] ) / 'Rear track' [mm])" SampleRate="0" Unit="rad"/>
  <MathExpression Id="Average Front tire temp" DisplayUnit="C" DisplayDPS="2" DisplayColorIndex="6" Interpolate="1" Script="( choose('Tire Temperature Front Left Average' [C] &lt;= 150, 'Tire Temperature Front Left Average' [C], invalid()) + choose('Tire Temperature Front Right Average' [C] &lt;= 150, 'Tire Temperature Front Right Average' [C], invalid()) ) / ( choose('Tire Temperature Front Left Average' [C] &lt;= 150, 1, 0) + choose('Tire Temperature Front Right Average' [C] &lt;= 150, 1, 0) )" SampleRate="0" Unit="C"/>
  <MathExpression Id="Average Rear tire temp" DisplayUnit="C" DisplayDPS="2" DisplayColorIndex="1" Interpolate="1" Script="( choose('Tire Temperature Rear Left Average' [C] &lt;= 150, 'Tire Temperature Rear Left Average' [C], invalid()) + choose('Tire Temperature Rear Right Average' [C] &lt;= 150, 'Tire Temperature Rear Right Average' [C], invalid()) ) / ( choose('Tire Temperature Rear Left Average' [C] &lt;= 150, 1, 0) + choose('Tire Temperature Rear Right Average' [C] &lt;= 150, 1, 0) )" SampleRate="0" Unit="C"/>
  <MathExpression Id="Steering Angle Speed" DisplayUnit="deg/s" DisplayDPS="2" DisplayColorIndex="7" Interpolate="1" Script="derivative('Steering Angle' [deg])" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Steering Intergral" DisplayUnit="deg" DisplayDPS="2" DisplayColorIndex="5" Interpolate="1" Script="integrate(abs('Steering Angle' [deg]), 'Corr Speed' [km/h] &gt;= 5 AND 'Engine Speed' [rpm] &gt;= 3000, range_change(&quot;Outings:Laps&quot;))" SampleRate="0" Unit="deg"/>
  <MathExpression Id="Rear Temp Diffrance" DisplayUnit="C" DisplayDPS="2" DisplayColorIndex="0" Interpolate="1" Script="'Tire Temperature Rear Right Average' [C] - 'Tire Temperature Rear Left Average' [C]" SampleRate="0" Unit="C"/>
  <MathExpression Id="RR Wheel movement" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="7" Interpolate="1" Script="'RR damper postion Smoothed' [mm] * 'Motion Ratio' []" SampleRate="0" Unit="mm"/>
  <MathExpression Id="RL Wheel movement" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="4" Interpolate="1" Script="'RL damper postion Smoothed' [mm] * 'Motion Ratio' []" SampleRate="0" Unit="mm"/>
  <MathRegression Name="Front roll Gradient" X="Lateral accelration smoothed" Y="Front roll angle" Type="0" Order="2" Range="1" RangeGroupName="Outings:Laps" ZeroIntercept="0">
   <Outputs>
    <Output Enabled="0" Degree="0" Suffix="B"/>
    <Output Enabled="1" Degree="1" Suffix="Value"/>
    <Output Enabled="1" Degree="-1" Suffix="Hyst"/>
   </Outputs>
  </MathRegression>
  <MathRegression Name="Rear roll Gradient" X="Lateral accelration smoothed" Y="Rear roll angle" Type="0" Order="2" Range="1" RangeGroupName="Outings:Laps" ZeroIntercept="0">
   <Outputs>
    <Output Enabled="0" Degree="0" Suffix="B"/>
    <Output Enabled="1" Degree="1" Suffix="Value"/>
    <Output Enabled="1" Degree="-1" Suffix="Hyst"/>
   </Outputs>
  </MathRegression>
  <MathExpression Id="Front roll Speed" DisplayUnit="deg/s" DisplayDPS="2" DisplayColorIndex="1" Interpolate="1" Script="derivative('Front roll angle' [deg])" SampleRate="0" Unit="deg/s"/>
  <MathExpression Id="Rear Roll speed" DisplayUnit="deg/s" DisplayDPS="2" DisplayColorIndex="6" Interpolate="1" Script="derivative('Rear roll angle' [deg])" SampleRate="0" Unit="deg/s"/>
  <MathRegression Name="Front Roll Angle" X="Corr Speed" Y="Front roll angle" Type="0" Order="2" Range="1" RangeGroupName="Outings:Laps" ZeroIntercept="0">
   <Outputs>
    <Output Enabled="1" Degree="0" Suffix="Offset"/>
    <Output Enabled="0" Degree="1" Suffix="A"/>
    <Output Enabled="0" Degree="-1" Suffix="R2"/>
   </Outputs>
  </MathRegression>
  <MathExpression Id="Corr Front Roll angle" DisplayUnit="deg" DisplayDPS="2" DisplayColorIndex="0" Interpolate="1" Script="'Front roll angle' [deg] - 'Front Roll Angle.Offset'" SampleRate="0" Unit="deg"/>
  <MathExpression Id="Front Heave Damper Postion Smoothed" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="6" Interpolate="1" Script="smooth('Suspension Damper Position Front Heave' [mm], 10.0)" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Front Roll Damper Postion Smoothed" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="0" Interpolate="1" Script="smooth('Suspension Damper Position Front Roll' [mm], 10)" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Rear Heave Damper Postion Smoothed" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="4" Interpolate="1" Script="smooth('Suspension Damper Position Rear Heave' [mm], 15.0)" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Rear Roll Damper Postion Smoothed" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="5" Interpolate="1" Script="smooth('Suspension Damper Position Rear Roll' [mm], 10)" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Front Heave Damper Travel" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="1" Interpolate="1" Script="'Front Heave Damper Unloaded Length' [mm] - 'Front Heave Damper Postion Smoothed' [mm]" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Smooth Front Heave Damper Velocity" DisplayUnit="mm/s" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="derivative('Front Heave Damper Travel' [mm])" SampleRate="0" Unit="mm/s"/>
  <MathExpression Id="Rear Roll Damper Travel" DisplayUnit="mm" DisplayDPS="2" DisplayColorIndex="5" Interpolate="1" Script="'Rear Roll Damper Unloaded Length' [mm] - 'Suspension Damper Position Rear Roll' [mm]" SampleRate="0" Unit="mm"/>
  <MathExpression Id="Rear Roll Damper Velocity" DisplayUnit="mm/s" DisplayDPS="2" DisplayColorIndex="5" Interpolate="1" Script="derivative('Front Heave Damper Travel' [mm])" SampleRate="0" Unit="mm/s"/>
 </MathItems>
</Maths>
