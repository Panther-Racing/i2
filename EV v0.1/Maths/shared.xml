<?xml version="1.0"?>
<Maths Locale="English_United States.1252" DefaultLocale="C" Id="shared">
 <MathConstants>
  <MathConstant Name="Acceleration smoothing" Value="20" Unit="" Value.Min="0" Value.Max="100" QuickEdit="1"/>
  <MathConstant Name="PI" Value="3.14159265" Unit=""/>
 </MathConstants>
 <MathItems>
  <MathExpression Id="Instantanius Corner Radius" DisplayUnit="m" DisplayDPS="2" DisplayColorIndex="1" Interpolate="1" Script="choose(( ( ( 'Corr Speed' [m/s] ) ^ 2 ) / abs('Lateral accelration smoothed' [m/s/s]) ) &lt;= 50, ( ( 'Corr Speed' [m/s] ) ^ 2 ) / abs('Lateral accelration smoothed' [m/s/s]), invalid())" SampleRate="0" Unit="m"/>
  <MathExpression Id="Lateral accelration smoothed" DisplayUnit="G" DisplayDPS="2" DisplayColorIndex="6" Interpolate="1" Script="smooth('Vehicle Acceleration Lateral' [G], 'Acceleration smoothing' [])" SampleRate="0" Unit="G"/>
  <MathExpression Id="Longitudnal accelration smoothed" DisplayUnit="G" DisplayDPS="2" DisplayColorIndex="2" Interpolate="1" Script="smooth('Vehicle Acceleration Longitudinal' [G], 'Acceleration smoothing' [])" SampleRate="0" Unit="G"/>
  <MathExpression Id="Vertical accelration smoothed " DisplayUnit="G" DisplayDPS="2" DisplayColorIndex="3" Interpolate="1" Script="smooth('Vehicle Acceleration Vertical' [G], 'Acceleration smoothing' [])" SampleRate="0" Unit="G"/>
  <MathExpression Id="inverse radius" DisplayUnit="1/m" DisplayDPS="2" DisplayColorIndex="1" Interpolate="1" Script="choose(( ( 'Corr Speed' [m/s] ) ^ 2 ) / abs('Lateral accelration smoothed' [m/s/s]) &gt;= 5, abs('Lateral accelration smoothed' [m/s/s]) / ( ( 'Corr Speed' [m/s] ) ^ 2 ), invalid())" SampleRate="0" Unit="1/m"/>
 </MathItems>
</Maths>
